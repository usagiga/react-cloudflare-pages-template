{
  "name": "%REPO_NAME%",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.6",
    "pnpm": ">=8"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "",
    "build": "",
    "test": "playwright test -c playwright-ct.config.ts",
    "format": "prettier --list-different 'src/**/*.{js,jsx,ts,tsx,gql,graphql,json,css,scss}'",
    "format:fix": "prettier --write ./src",
    "lint": "pnpm run lint:es && pnpm run lint:style",
    "lint:fix": "pnpm run lint:es:fix && pnpm run lint:style:fix",
    "lint:es": "eslint --max-warnings=0 './src/**/*.{js,jsx,ts,tsx}'",
    "lint:es:fix": "eslint --max-warnings=0 --fix './src/**/*.{js,jsx,ts,tsx}'",
    "lint:style": "stylelint --max-warnings=0 'src/**/*.{css,less,sass,scss}'",
    "lint:style:fix": "stylelint --max-warnings=0 --fix 'src/**/*.{css,less,sass,scss}'",
    "prepare": "npx only-allow pnpm && husky install && playwright install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.56",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "husky": "^8.0.1",
    "postcss": "^8.4.18",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "sass": "^1.54.5",
    "stylelint": "^14.11.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^28.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.8.4"
  }
}
